<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mobile.travelaja">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-feature
        android:name="android.hardware.location"
        android:required="true" />
    <uses-feature
        android:name="android.hardware.location.gps"
        android:required="true" />
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:name="com.mobile.travelaja.base.InitApplications"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:hardwareAccelerated="true"
        android:theme="@style/AppTheme"
        android:requestLegacyExternalStorage="true"
        android:usesCleartextTraffic="true"
        
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="com.mobile.travelaja.module.settlement.SettlementActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.settlement.TripSearchActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.create_trip.newtrip_pertamina.activity.CityActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map" />


        <activity
            android:name="com.mobile.travelaja.module.signin.splash.activity.SplashActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.signin.login.activity.AddressActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.mobile.travelaja.module.item_custom.map.TestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.accomodation.view_accomodation.activity.AccomodationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.item_custom.galery.MyGaleryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.profile.SimFormContactActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.profile.KtpCardFormActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.profile.PassportFormActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.signin.login.activity.LoginPremiumActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.profile.OtherFormActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.cart.activity.NewCartActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.signin.login.activity.LoginActivity"
            android:screenOrientation="portrait">

            <!-- this main -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mobile.travelaja.module.signin.login.activity.OtpRegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.item_custom.calendar.NewCalendarViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.home.activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.signin.select_nationality.activity.SelectNationalityActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.create_trip.newtrip_pertamina.dialog.DialogPurpose"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.signin.select.activity.LookUpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.signin.detail_profile.activity.CompletedDataProfileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.signin.detail_profile.activity.DataProfileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.item_custom.calendar.CalendarViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.my_booking.purchase_detail.PurchaseDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.my_booking.purchase_list_detail.PurchaseDetailListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.profile.JoinTripActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.item_custom.map.MapActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.approval.activity.DetailTripActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.approval.activity.DetailParticipantActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.create_trip.newtrip.actvity.CreateTripActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.yalantis.ucrop.UCropActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.mobile.travelaja.module.create_trip.select_budget.activity.SelectBudget"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.approval.activity.DetailApprovalActivityOld"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.create_trip.newtrip_travelaja.CreateTripTravelAjaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.create_trip.newtrip_pertamina.activity.CreateTripPertaminaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.create_trip.newtrip_pertamina.activity.SelectTripRoutePertaminaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.create_trip.newtrip_pertamina.activity.RevieBudgetPertaminaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.signin.register.ComplitelyRegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.signin.register.RegisterWithEmailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.item_custom.success_view.SuccessView"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.locale.ApplyLanguageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.create_trip.success_create_trip.SucessCreateTripPlaneActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.payment.PaymentActivity"
            android:screenOrientation="portrait" />

        <receiver
            android:name="com.mobile.travelaja.locale.AppLocaleChangeReceiver"
            android:enabled="true"
            android:exported="true" />

        <meta-data
            android:name="firebase_crashlytics_collection_enabled"
            android:value="false" />

        <activity
            android:name="com.mobile.travelaja.module.notif.YesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.module.home.activity.HomeWebActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mobile.travelaja.base.SearchActivity"
            android:screenOrientation="portrait" />

        <service android:name="com.mobile.travelaja.module.notif.CorpFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <receiver
            android:name="com.mobile.travelaja.module.notif.NotificationReceiver"
            android:enabled="true"
            android:exported="false" />

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />

        <activity
            android:name="net.openid.appauth.RedirectUriReceiverActivity" tools:node="replace">
            <intent-filter android:autoVerify="true">
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="${applicationId}" />
            </intent-filter>
        </activity>
    </application>
</manifest>