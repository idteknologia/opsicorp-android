<?xml version="1.0" encoding="utf-8"?>

<resources>
    <attr name="siArrowPosition">
        <enum name="left" value="0" />
        <enum name="right" value="1" />
    </attr>
    <attr name="siBorderType">
        <enum name="stroke" value="0" />
        <enum name="fill" value="1" />
    </attr>
    <attr name="siStrokeCap">
        <enum name="butt" value="0" />
        <enum name="round" value="1" />
        <enum name="square" value="2" />
    </attr>
    <attr name="siStrokeJoin">
        <enum name="bevel" value="0" />
        <enum name="miter" value="1" />
        <enum name="round" value="2" />
    </attr>

    <declare-styleable name="ShaderImageView">
        <attr name="siSquare" format="boolean" />
        <attr name="siBorderColor" format="color" />
        <attr name="siBorderWidth" format="dimension" />
        <attr name="siBorderAlpha" format="float" />
        <attr name="siForeground" format="integer|reference" />
        <!-- Rounded Image View -->
        <attr name="siRadius" format="dimension" />
        <!--BubbleImageView-->
        <attr name="siArrowPosition"/>
        <attr name="siTriangleHeight" format="dimension"/>
        <!--PorterImageView-->
        <attr name="siShape" format="integer|reference" />
        <!--ShaderImageView-->
        <attr name="siBorderType"/>
        <attr name="siStrokeCap"/>
        <attr name="siStrokeJoin"/>
        <attr name="siStrokeMiter" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ColorOptionsView">
        <attr name="titleText" format="string" localization="suggested" />
        <attr name="valueColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ToolbarOpsicorp">
        <attr name="toolbar_hidden_cart" format="boolean" />
        <attr name="toolbar_hidden_title"  format="boolean" />
        <attr name="toolbar_hidden_back"  format="boolean" />
    </declare-styleable>

    <declare-styleable name="ButtonDefaultOpsicorp">
        <attr name="text" format="string" />
<!--        <attr name="backgroundButton" format="reference|color" />-->
    </declare-styleable>

    <declare-styleable name="expandableLayout">
        <attr name="ael_duration" format="integer" />
        <attr name="ael_expanded" format="boolean" />
        <attr name="ael_defaultChildIndex" format="integer" />
        <attr name="ael_defaultPosition" format="dimension" />
        <attr name="ael_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="ael_interpolator" format="enum">
            <enum name="accelerateDecelerate" value="0" />
            <enum name="accelerate" value="1" />
            <enum name="anticipate" value="2" />
            <enum name="anticipateOvershoot" value="3" />
            <enum name="bounce" value="4" />
            <enum name="decelerate" value="5" />
            <enum name="fastOutLinearIn" value="6" />
            <enum name="fastOutSlowIn" value="7" />
            <enum name="linear" value="8" />
            <enum name="linearOutSlowIn" value="9" />
            <enum name="overshoot" value="10" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TicketView">
        <attr name="ticketOrientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="ticketBackgroundColor" format="color" />
        <attr name="ticketScallopRadius" format="dimension" />
        <attr name="ticketScallopPositionPercent" format="float" />
        <attr name="ticketShowBorder" format="boolean" />
        <attr name="ticketBorderWidth" format="dimension" />
        <attr name="ticketBorderColor" format="color" />
        <attr name="ticketShowDivider" format="boolean" />
        <attr name="ticketDividerWidth" format="dimension" />
        <attr name="ticketDividerPadding" format="dimension" />
        <attr name="ticketDividerColor" format="color" />
        <attr name="ticketDividerType" format="enum">
            <enum name="normal" value="0" />
            <enum name="dash" value="1" />
        </attr>
        <attr name="ticketDividerDesign" format="enum">
            <enum name="color" value="0" />
            <enum name="punch" value="1" />
        </attr>
        <attr name="ticketDividerDashGap" format="dimension" />
        <attr name="ticketDividerDashLength" format="dimension" />
        <attr name="ticketCornerType" format="enum">
            <enum name="normal" value="0" />
            <enum name="rounded" value="1" />
            <enum name="scallop" value="2" />
        </attr>
        <attr name="ticketCornerRadius" format="dimension" />
        <attr name="android:elevation" />
        <attr name="ticketElevation" format="dimension" />
        <attr name="view" format="reference" />
        <attr name="ticketDividerLocation">
            <flag name="none" value="0" />
            <flag name="top" value="1" />
            <flag name="bottom" value="2" />
            <flag name="left" value="4" />
            <flag name="right" value="8" />
        </attr>

        <attr name="ticketPunchLocation">
            <flag name="none" value="0" />
            <flag name="top" value="1" />
            <flag name="bottom" value="2" />
            <flag name="left" value="4" />
            <flag name="right" value="8" />
        </attr>
        <attr name="ticketPunchRadius" format="dimension" />

    </declare-styleable>

    <declare-styleable name="WheelPicker">
        <attr name="scroll_data" format="reference" />
        <attr name="scroll_background_color" format="color" />
        <attr name="scroll_selected_item_background" format="color" />
        <attr name="scroll_selected_item_position" format="integer" />
        <attr name="scroll_item_text_size" format="dimension" />
        <attr name="scroll_item_text_color" format="color" />
        <attr name="scroll_selected_item_text_color" format="color" />
        <attr name="scroll_maximum_width_text" format="string" />
        <attr name="scroll_maximum_width_text_position" format="integer" />
        <attr name="scroll_visible_item_count" format="integer" />
        <attr name="scroll_item_space" format="dimension" />
        <attr name="scroll_indicator" format="boolean" />
        <attr name="scroll_indicator_color" format="color" />
        <attr name="scroll_indicator_size" format="dimension" />
        <attr name="scroll_atmospheric" format="boolean" />
        <attr name="scroll_item_align" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ViewNavigator">
        <attr name="layoutId" format="reference" />
    </declare-styleable>
</resources>