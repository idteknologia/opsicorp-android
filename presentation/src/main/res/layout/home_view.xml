<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:custom="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fitsSystemWindows="true"
    android:background="@color/white"
    android:orientation="vertical"
    android:id="@+id/rootView">

    <!--<android.support.v4.widget.NestedScrollView
        android:visibility="visible"
        android:fillViewport="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent">



    </android.support.v4.widget.NestedScrollView>-->

    <FrameLayout
        android:id="@+id/place_fragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/contentButtonNav">
    </FrameLayout>

    <LinearLayout
        android:id="@+id/llProgress"
        android:visibility="gone"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rouded_border_loading"
        app:layout_constraintBottom_toBottomOf="@+id/place_fragment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/place_fragment">

        <ProgressBar
            android:id="@+id/progressBar"
            style="?android:attr/progressBarStyle"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp"
            android:visibility="gone" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/contentButtonNav"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.mobile.travelaja.module.item_custom.menu_bottom.MenuBottomOpsicorp
            android:layout_width="match_parent"
            android:id="@+id/menu_bottom"
            android:layout_height="wrap_content">
        </com.mobile.travelaja.module.item_custom.menu_bottom.MenuBottomOpsicorp>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>