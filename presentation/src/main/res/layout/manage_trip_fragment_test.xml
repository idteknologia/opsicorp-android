<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/nested_view"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:fillViewport="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/llTripHistory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="20dp"
            android:layout_gravity="end"
            android:orientation="horizontal"
            android:visibility="visible">
            <ImageView
                android:id="@+id/ivTripHistory"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginEnd="8dp"
                android:src="@drawable/ic_history"/>
            <TextView
                android:id="@+id/tvTripHistory"
                style="@style/TextLato_Bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textSize="18sp"
                android:textColor="#b324253d"
                android:text="@string/trip_history"
                />

        </LinearLayout>

        <TextView
            android:layout_marginTop="30dp"
            style="@style/RobotoMedium"
            android:layout_marginLeft="@dimen/margin_default"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="24sp"
            android:textColor="@color/tintTextView"
            android:text="@string/manage_trip"
            />

        <TextView
            style="@style/RobotoRegular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:layout_marginBottom="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="@dimen/margin_default"
            android:textColor="#939396"
            android:lineSpacingMultiplier="1"
            android:lineSpacingExtra="4dp"
            android:text="@string/yourtrip_hasbeen_released"/>

<!--        <com.opsigo.opsicorp.module.item_custom.button_top.ButtonTopRoundedOpsicorp-->
<!--            android:layout_width="match_parent"-->
<!--            android:id="@+id/top_button"-->
<!--            android:layout_marginTop="20dp"-->
<!--            android:layout_marginBottom="0dp"-->
<!--            android:layout_height="wrap_content">-->
<!--        </com.opsigo.opsicorp.module.item_custom.button_top.ButtonTopRoundedOpsicorp>-->

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <!--<include-->
            <!--android:emplaoyId="@+emplaoyId/line_list_approval"-->
            <!--android:visibility="visible"-->
            <!--android:layout_height="match_parent"-->
            <!--android:layout_width="match_parent"-->
            <!--layout="@layout/list_layout_approval">-->

            <!--</include>-->

<!--            <com.opsigo.opsicorp.module.item_custom.dasboard_approval.DasboardListApproval-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_height="match_parent"-->
<!--                android:id="@+id/line_list_approval">-->
<!--            </com.opsigo.opsicorp.module.item_custom.dasboard_approval.DasboardListApproval>-->

            <com.mobile.travelaja.module.manage_trip.TestDasboardListApproval
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/line_list_approval">
            </com.mobile.travelaja.module.manage_trip.TestDasboardListApproval>

            <!--<include-->
